
# Maintainer: WinterMute <davem@devkitpro.org>
pkgname=gp2x-pkg-config
pkgver=0.29
pkgrel=1
pkgdesc='pkg-config wrapper (for GP2X homebrew development)'
arch=('any')
url='http://devkitpro.org/'
source=('pkg-config.in')
sha256sums=('11a50121b620ba57e64f453c0c32f632149a9b831e8968058be4a5d14177c527')
groups=('gp2x-dev')

package() {
  install -d "$pkgdir$DEVKITPRO"/portlibs/gp2x/bin
  cp "$srcdir"/pkg-config.in "$pkgdir$DEVKITPRO"/portlibs/gp2x/bin/arm-none-eabi-pkg-config
}
